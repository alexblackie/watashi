title: RSS Feed
slug: /feed.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>Alex Blackie's Blog</title>
		<link>{{ permalink "/" }}</link>
		<description>An RSS feed for all my published articles.</description>
		<language>en-GB</language>
		<atom:link href="{{ permalink "/feed.xml" }}" rel="self" type="application/rss+xml" />

		{{ range .Assigns.Articles }}
		<item>
			<title>{{ .Document.Meta.Title }}</title>
			<link>{{ permalink .Document.Meta.Slug }}</link>
			<guid>{{ permalink .Document.Meta.Slug }}</guid>
			<pubDate>{{ rfc822Date .Document.Meta.PublishDate }}</pubDate>
			<author>alex@alexblackie.com (Alex Blackie)</author>
			<description></description>
			<content:encoded><![CDATA[ {{ .Output }} ]]></content:encoded>
		</item>
		{{ end }}

	</channel>
</rss>
